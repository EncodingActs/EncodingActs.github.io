###  http://EncodingActs.github.io/MAont/MaOntology/Form_set
mao:Form_set rdf:type owl:Class ;
             rdfs:subClassOf mao:class:training_method ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty mao:assessed_by ;
                               owl:someValuesFrom mao:class:grading_system
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty mao:belongs_to ;
                               owl:someValuesFrom mao:class:MA_system
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty mao:contains ;
                               owl:someValuesFrom mao:class:MA_form
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty mao:practiced_in ;
                               owl:someValuesFrom mao:class:MA_style
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty mao:similar_set_of ;
                               owl:someValuesFrom mao:Form_set
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty mao:type_of ;
                               owl:someValuesFrom mao:class:training_method
                             ] ;
